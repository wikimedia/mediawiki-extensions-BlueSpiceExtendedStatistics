{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"parserOptions": {
		"sourceType": "commonjs"
	},
	"globals": {
		"blueSpice": "readonly",
		"bs": "readonly",
		"jQuery": "readonly",
		"mediaWiki": "readonly",
		"OOJSPlus": "readonly"
	},
	"rules": {
		"es-x/no-object-values": "off",
		"max-len": "off",
		"no-jquery/no-global-selector": "off",
		"no-prototype-builtins": "off"
	}
}
